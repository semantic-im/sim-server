:IdleCPULoad rdf:type owl:Class ;
             
             rdfs:subClassOf :SystemMetric ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasMeasurementUnit ;
                               owl:hasValue :Percent
                             ] ,
