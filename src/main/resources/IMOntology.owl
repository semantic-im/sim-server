:IOWrite rdf:type owl:Class ;
         
         rdfs:subClassOf :SystemMetric ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasMeasurementUnit ;
                           owl:hasValue :Byte
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasDataValue ;
                           owl:allValuesFrom xsd:long
                         ] ;
         
         <http://purl.org/dc/elements/1.1/description> "I/O write" .



