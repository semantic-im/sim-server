           rdfs:subClassOf :MetricType ;
           
           <http://purl.org/dc/elements/1.1/description> "An exceptional condition occurred while processing the request" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#IORead

:IORead rdf:type owl:Class ;
        
        rdfs:subClassOf :SystemMetric ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDataValue ;
                          owl:allValuesFrom xsd:long
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasMeasurementUnit ;
                          owl:hasValue :Byte
                        ] ;
        
        <http://purl.org/dc/elements/1.1/description> "I/O read" .



