:MeasurementUnit rdf:type owl:Class ,
		 owl:NamedIndividual ;
                 dc:description "concept that represents an abstract measurement unit" .

:Microsecond rdf:type :MeasurementUnit ;
	     rdfs:label "us" .

:Millisecond rdf:type :MeasurementUnit ;
	rdfs:label "ms" .

:Second rdf:type :MeasurementUnit ;
	rdfs:label "s" .

:Percent rdf:type :MeasurementUnit ;
	rdfs:label "%" .

:Byte rdf:type :MeasurementUnit ;
	rdfs:label "B" .

:Kilobyte rdf:type :MeasurementUnit ;
	rdfs:label "KB" .

:Counter rdf:type :MeasurementUnit ;
	rdfs:label "count" .

:TimePoint rdf:type :MeasurementUnit ;
	rdfs:label "tp" ;
	rdfs:comment "the difference, measured in milliseconds, between the current time and midnight, January 1, 1970 UTC." .

:ErrorStatus rdf:type :MeasurementUnit ;
	rdfs:label "error status" ;
	rdfs:comment "a flag that signals if there was an error" .

:ErrorMessage rdf:type :MeasurementUnit ;
	rdfs:label "error message" ;
	rdfs:comment "exception type and exception error message" .

:StringValue rdf:type :MeasurementUnit ;
	rdfs:label "string value" ;
	rdfs:comment "a text message" .

:NumericValue rdf:type :MeasurementUnit ;
	rdfs:label "numeric value" ;
	rdfs:comment "a numeric value" .

:BooleanValue rdf:type :MeasurementUnit ;
	rdfs:label "boolean value" ;
	rdfs:comment "a boolean value" .


:hasTimeStamp rdf:type owl:DatatypeProperty ;
              dc:description "specifies the time stamp when the measurment was performed" ;
              rdfs:range xsd:dateTime ;
