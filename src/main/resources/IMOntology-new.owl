###  http://www.larkc.eu/ontologies/IMOntology.rdf#QuerySuccessRatePerTimeInterval

:QuerySuccessRatePerTimeInterval rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :PerTimeInterval ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasAggregationFunction ;
                                                   owl:allValuesFrom :Count
                                                 ] ;
                                 
                                 <http://purl.org/dc/elements/1.1/description> "The number of queries that finished with success" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#Result

:Result rdf:type owl:Class ;
        
        rdfs:subClassOf owl:Thing ;
        
        <http://purl.org/dc/elements/1.1/description> "concept that represents the result of a SPARQL query" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#ResultSize

:ResultSize rdf:type owl:Class ;
            
            rdfs:subClassOf :Metric ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDataValue ;
                              owl:allValuesFrom xsd:integer
                            ] ;
            
            <http://purl.org/dc/elements/1.1/description> "concept that represents the size of a result" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#ResultSizeInBytes

:ResultSizeInBytes rdf:type owl:Class ;
                   
                   rdfs:subClassOf :ResultSize ;
                   
                   <http://purl.org/dc/elements/1.1/description> "the size of a result represented in bytes" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#ResultSizeInTriples

:ResultSizeInTriples rdf:type owl:Class ;
                     
                     rdfs:subClassOf :ResultSize ;
                     
                     <http://purl.org/dc/elements/1.1/description> "the size of a result represented in terms of number of RDF triples" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#Session

:Session rdf:type owl:Class ;
         
         <http://purl.org/dc/elements/1.1/description> "Generic concept of a session" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#Size

:Size rdf:type owl:Class ;
      
      rdfs:subClassOf :MetricType ;
      
      <http://purl.org/dc/elements/1.1/description> "The size of the data structure" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#Sum

:Sum rdf:type owl:Class ;
     
     rdfs:subClassOf :AggregationFunction .




###  http://www.larkc.eu/ontologies/IMOntology.rdf#System

:System rdf:type owl:Class ;
        
        <http://purl.org/dc/elements/1.1/description> "concept that represents a system where the application is running (e.g. your personal computer)" .




###  http://www.larkc.eu/ontologies/IMOntology.rdf#TimeInterval

:TimeInterval rdf:type owl:Class ;
              
              <http://purl.org/dc/elements/1.1/description> "represents a time interval between two moments in time" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#Variable

:Variable rdf:type owl:Class ;
          
          <http://purl.org/dc/elements/1.1/description> "concept that represents a variable from a SPARQL query" .




###  http://www.larkc.eu/ontologies/IMOntology.rdf#Workflow

:Workflow rdf:type owl:Class ;
          
          rdfs:subClassOf :Context ;
          
          <http://purl.org/dc/elements/1.1/description> "concept that represents a LarKC workflow" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowAvgDurationPerTimeInterval

:WorkflowAvgDurationPerTimeInterval rdf:type owl:Class ;
                                    
                                    rdfs:subClassOf :WorkflowAvgPerTimeInterval ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :includes ;
                                                      owl:onClass :WorkflowTotalResponseTime ;
                                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasMeasurementUnit ;
                                                      owl:hasValue :Microsecond
                                                    ] ;
                                    
                                    <http://purl.org/dc/elements/1.1/description> "The average duration of a given work-flow in a given time interval" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowAvgNodesPerTimeInterval

:WorkflowAvgNodesPerTimeInterval rdf:type owl:Class ;
                                 
                                 rdfs:subClassOf :WorkflowAvgPerTimeInterval ;
                                 
                                 <http://purl.org/dc/elements/1.1/description> "The average number of nodes used by a given work-flow in a given time interval" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowAvgPerTimeInterval

:WorkflowAvgPerTimeInterval rdf:type owl:Class ;
                            
                            rdfs:subClassOf :PerTimeInterval ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :hasAggregationFunction ;
                                              owl:allValuesFrom :Average
                                            ] .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowAvgThreadsPerTimeInterval

:WorkflowAvgThreadsPerTimeInterval rdf:type owl:Class ;
                                   
                                   rdfs:subClassOf :WorkflowAvgPerTimeInterval ;
                                   
                                   <http://purl.org/dc/elements/1.1/description> "The average number of threads used by a given work-flow in a given time interval" .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowCreation

:WorkflowCreation rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Workflow .




###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowExecution

:WorkflowExecution rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Workflow .



###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowInvocation

:WorkflowInvocation rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Event .




###  http://www.larkc.eu/ontologies/IMOntology.rdf#WorkflowsPerTimeInterval

:WorkflowsPerTimeInterval rdf:type owl:Class ;
                          
                          rdfs:subClassOf :PerTimeInterval ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :hasAggregationFunction ;
                                            owl:allValuesFrom :Count
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :includes ;
                                            owl:onClass :WorkflowInvocation ;
                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                          ] ;
                          
                          <http://purl.org/dc/elements/1.1/description> "The number of work-flows of a given name that were started in a given time interval" .




